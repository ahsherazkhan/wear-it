{
  "manifest_version": 3,
  "name": "Clothing Try-On",
  "version": "1.0",
  "description": "Try on clothes using your uploaded images",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo192.png",
      "128": "logo512.png"
    }
  },
  "permissions": ["activeTab"],
  "icons": {
    "16": "logo.png",
    "48": "logo192.png",
    "128": "logo512.png"
  }
}
